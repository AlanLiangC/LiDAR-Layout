    "version": "0.2.0",
    "configurations": [

        {
            "name": "Python Debugger: Current File",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "cwd": "${workspaceFolder}/scripts",
            "args": [
                // train layout diffusion
                "-b", "../configs/layout_diffusion/nuscenes/layout_nusc.yaml",
                "-t",
                "--gpus", "1",
                "--batch_size", "2",
                "--num_workers", "1"
            ]

        }
    ]
}